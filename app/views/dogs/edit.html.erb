<div class="bodydogsedit"></div>
<div class="graddogsedit"></div>

<h1 class="headerdogsedit">Actualiza a tu Perrito</h1>

<%# <div class="containerdogsedit"> %>
  <%# <div class="headerdivdogsedit"> %>
    <%# <ul class="datosdogsedit"> %>
      <div class="btndogsedit">
        <%= form_with model: @dog do |f| %>
          <%= f.label :Nombre %>
          <%= f.text_field :name %>
          <%= f.label :Cumpleaños %>
          <%= f.text_field :date_of_birth %>
          <%= f.label :Raza %>
          <%= f.text_field :breed %>
          <%= f.label :Tamaño %>
          <%= f.text_field :size %>
          <%= f.label :Vacunas %>
          <%= f.text_field :vaccines %>
          <%= f.label :Enfermedad %>
          <%= f.text_field :sickness_allergies %>
          <%= f.label :Descripcion %>
          <%= f.text_field :about_me %>
          <hr>
          <%= f.submit "Actualizar Perrito"%>
        <% end %>
      </div>
    </ul>
  </div>
</div>

<%= form_with model: @dog do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :date_of_birth %>
  <%= f.text_field :date_of_birth %>
  <%= f.label :breed %>
  <%= f.text_field :breed %>
  <%= f.label :size %>
  <%= f.text_field :size %>
  <%= f.label :vaccines %>
  <%= f.text_field :vaccines %>
  <%= f.label :sickness_allergies %>
  <%= f.text_field :sickness_allergies %>
  <%= f.label :about_me %>
  <%= f.text_field :about_me %>
  <%= f.submit %>
<% end %>